    "about_story_jAGMcw": {
      "type": "about-story",
      "name": "About Story",
      "settings": {
        "left_image_1": "shopify://shop_images/1.webp",
        "left_image_2": "shopify://shop_images/2.webp",
        "main_heading": "Our friendship spans more than twenty years. We started as college roommates in Boulder, then spent our twenties together in New York.",
        "main_text": "<p>Now we live on opposite coasts, but nothing’s really changed.</p>",
        "right_image_1": "shopify://shop_images/4.webp",
        "right_image_2": "shopify://shop_images/3.webp",
        "main_heading1": "We’ve spent our careers bringing ideas <br>to life—",
        "main_text2": "<p>building brands, leading creative teams, and crafting stories that connect. From advertising and art direction to design and operations, we’ve spent years refining how things look, feel, and come together.</p>"
      }
    },
    "about_story_second_DicGMr": {
      "type": "about-story-second",
      "name": "About Story Second",
      "settings": {
        "left_image_1": "shopify://shop_images/IonianCore-JPGtoWEBP-Converter.webp",
        "main_heading": "Our passports are filled with stamps from years of shared adventures.",
        "main_text": "<p>Our trips always lead us to the people and places where things are still made by hand. Give us a mercado, a block-printing studio, or an antique dealer’s shop filled with treasures, and we’re in our element. We are drawn to pattern, color, and pieces that feel alive—the kind that tell a story. </p>",
        "right_image_1": "shopify://shop_images/IonianCore-JPGtoWEBP-Converter-1.webp",
        "right_image_2": "shopify://shop_images/IonianCore-JPGtoWEBP-Converter-3.webp",
